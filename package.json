{
  "name": "pastebox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier components/ --write",
    "db:generate": "NODE_ENV=development dotenv -e .env.local -- drizzle-kit generate",
    "db:push": "NODE_ENV=development dotenv -e .env.local -- drizzle-kit push",
    "db:migrate": "NODE_ENV=development dotenv -e .env.local -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:dev:up": "docker compose -f docker-compose.dev.yml up -d",
    "db:dev:down": "docker compose -f docker-compose.dev.yml down -v",
    "docker:prod:build": "docker compose build --no-cache",
    "docker:prod:up": "docker compose up -d"
  },
  "dependencies": {
    "@lexical/code": "^0.37.0",
    "@lexical/link": "^0.37.0",
    "@lexical/list": "^0.37.0",
    "@lexical/markdown": "^0.37.0",
    "@lexical/react": "^0.37.0",
    "@lexical/rich-text": "^0.37.0",
    "@lexical/selection": "^0.37.0",
    "@lexical/text": "^0.37.0",
    "@lexical/utils": "^0.37.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "lexical": "^0.37.0",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.546.0",
    "next": "16.0.0",
    "next-themes": "^0.4.6",
    "omgopass": "^3.2.1",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "random-word-slugs": "^0.1.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "slugify": "^1.6.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "yazl": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20",
    "@types/omgopass": "^3.2.3",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/yazl": "^3.3.0",
    "@vercel/nft": "^0.30.3",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-n": "^17.23.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
